<?xml version="1.0" encoding="utf-8"?>

<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <com.example.focusablelistview.FocusableListView
        android:id="@+id/focusLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        app:centerScrollPosition="true"
        app:itemDecoration="16dp"
        app:itemHighLight="true"
        app:itemImageCornerRadius="10dp"
        app:orientation="horizontal"
        tools:context=".recycler.custom_focus.CustomFocusActivity"/>
</layout>